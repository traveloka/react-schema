{
  "name": "@traveloka/react-schema-form",
  "version": "3.4.4",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "lint": "tslint --type-check -p tsconfig.json src/*.tsx && tslint --type-check -p tsconfig.json tests/*.tsx",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn lint && yarn test:coverage -i --reporters jest-simple-dot-reporter jest-junit",
    "test:html": "nyc npm test && nyc report --reporter=html",
    "prebuild": "npm run clean",
    "clean": "rimraf lib/*",
    "build": "tsc -p .",
    "watch": "tsc -w -p .",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@traveloka/react-schema": "^3.4.4",
    "@traveloka/validation": "^3.4.4",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.120",
    "@types/react": "^16.4.8",
    "@types/react-dom": "^16.0.7",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-simple-dot-reporter": "^1.0.2",
    "nyc": "^15.1.0",
    "react": "^16.3.1",
    "react-test-renderer": "^16.3.2",
    "ts-jest": "^23.1.3",
    "tslint": "5.11.0",
    "typescript": "^3.9.9"
  },
  "license": "MIT",
  "jest": {
    "notify": true,
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
